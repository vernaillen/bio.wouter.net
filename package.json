{
  "version": "1.3.1",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt build && nuxt preview",
    "postinstall": "nuxt prepare",
    "typecheck": "nuxi typecheck",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "cy:install": "cypress install",
    "cy:dev": "start-server-and-test dev http://localhost:3000 'cypress open --config baseUrl=http://localhost:3000'",
    "cy:local": "start-server-and-test preview http://localhost:3000 'cypress run --config baseUrl=http://localhost:3000'",
    "fulltest": "rm -rf node_modules && pnpm i && pnpm run lint && pnpm run coverage && pnpm run cy:local"
  },
  "devDependencies": {
    "@iconify-json/arcticons": "^1.1.68",
    "@iconify-json/game-icons": "^1.1.3",
    "@iconify-json/logos": "^1.1.34",
    "@iconify-json/material-symbols": "^1.1.57",
    "@iconify-json/mdi": "^1.1.54",
    "@iconify-json/uil": "^1.1.5",
    "@nuxt/devtools": "^0.8.0",
    "@nuxt/image": "1.0.0-rc.1",
    "@nuxthq/ui": "^2.7.0",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@nuxtjs/plausible": "^0.2.1",
    "@vernaillen/nuxt-base-layer": "^0.2.18",
    "@vitest/coverage-v8": "^0.34.3",
    "@vueuse/nuxt": "^10.4.0",
    "consola": "^3.2.2",
    "cypress": "^12.17.4",
    "eslint": "^8.48.0",
    "happy-dom": "^10.11.0",
    "jsdom": "^22.1.0",
    "nuxt": "^3.7.0",
    "nuxt-multi-cache": "^3.1.1",
    "nuxt-vitest": "^0.10.2",
    "start-server-and-test": "^2.0.0",
    "typescript": "^5.2.2",
    "vitest": "0.34.3",
    "vue-tsc": "^1.8.8"
  }
}

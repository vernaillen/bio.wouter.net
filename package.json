{
  "name": "links.wouter.net",
  "version": "1.0.1",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && cross-env NODE_ENV=production vite-ssg build",
    "serve": "pnpm run build && serve dist",
    "preview": "vite preview --port 4173",
    "test": "vitest --environment jsdom",
    "coverage": "vitest run --coverage",
    "build-only": "vite-ssg build",
    "typecheck": "vue-tsc --noEmit --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "cy:install": "cypress install",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:dev": "start-server-and-test dev http://localhost:5173 'cypress open --config baseUrl=http://localhost:5173'",
    "cy:local": "start-server-and-test serve http://localhost:3000 'cypress run --config baseUrl=http://localhost:3000'",
    "fulltest": "rm -rf node_modules && pnpm i && pnpm run lint && pnpm run coverage && pnpm run cy:local"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-brands-svg-icons": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@fortawesome/vue-fontawesome": "^3.0.2",
    "animate.css": "^4.1.1",
    "pinia": "^2.0.28",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.33.1",
    "@pinia/testing": "^0.0.14",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.11.12",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vitest/coverage-c8": "^0.25.6",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.2.6",
    "@vue/tsconfig": "^0.1.3",
    "@vueuse/head": "^1.0.22",
    "cross-env": "^7.0.3",
    "cypress": "^12.0.2",
    "eslint": "^8.29.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.8.0",
    "jsdom": "^20.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.1",
    "start-server-and-test": "^1.15.2",
    "typescript": "^4.9.4",
    "vite": "^4.0.0",
    "vite-ssg": "^0.22.0",
    "vitest": "^0.25.6",
    "vue-router": "^4.1.6",
    "vue-tsc": "^1.0.12"
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
}

{
  "name": "links.wouter.net",
  "version": "1.0.1",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && cross-env NODE_ENV=production vite-ssg build",
    "serve": "pnpm run build && serve dist",
    "preview": "vite preview --port 4173",
    "test": "vitest --environment jsdom",
    "coverage": "vitest run --coverage",
    "build-only": "vite-ssg build",
    "typecheck": "vue-tsc --noEmit --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "cypress:install": "cypress install",
    "cypress:open": "cypress open --config baseUrl=http://localhost:5173",
    "cypress:run": "cypress run",
    "cypress:serve": "cypress run --config baseUrl=http://localhost:3000",
    "e2e": "start-server-and-test dev http://localhost:5173 cypress:open",
    "e2e:serve": "start-server-and-test serve http://localhost:3000 cypress:serve",
    "fulltest": "rm -rf node_modules && pnpm i && pnpm run lint && pnpm run coverage && pnpm run cypress:local",
    "prepushtest": "rm -rf node_modules && pnpm i && pnpm run lint && pnpm run type-check && pnpm run build && serve dist"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/free-brands-svg-icons": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@fortawesome/vue-fontawesome": "^3.0.1",
    "animate.css": "^4.1.1",
    "pinia": "^2.0.23",
    "vue": "^3.2.41"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.27.0",
    "@pinia/testing": "^0.0.14",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^18.11.7",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vitejs/plugin-vue-jsx": "^2.1.0",
    "@vitest/coverage-c8": "^0.24.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.2.1",
    "@vue/tsconfig": "^0.1.3",
    "@vueuse/head": "^0.9.8",
    "cross-env": "^7.0.3",
    "cypress": "^10.11.0",
    "eslint": "^8.26.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.6.0",
    "jsdom": "^20.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "start-server-and-test": "^1.14.0",
    "typescript": "^4.8.4",
    "vite": "^3.2.1",
    "vite-ssg": "^0.21.1",
    "vitest": "^0.24.3",
    "vue-router": "^4.1.6",
    "vue-tsc": "^1.0.9"
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
}

{
  "version": "1.3.1",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt build && nuxt preview",
    "postinstall": "nuxt prepare",
    "typecheck": "nuxi typecheck",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "cy:install": "cypress install",
    "cy:dev": "start-server-and-test dev http://localhost:3000 'cypress open --config baseUrl=http://localhost:3000'",
    "cy:local": "start-server-and-test preview http://localhost:3000 'cypress run --config baseUrl=http://localhost:3000'",
    "fulltest": "rm -rf node_modules && pnpm i && pnpm run lint && pnpm run coverage && pnpm run cy:local"
  },
  "devDependencies": {
    "@iconify-json/arcticons": "^1.1.72",
    "@iconify-json/circle-flags": "^1.1.43",
    "@iconify-json/game-icons": "^1.1.3",
    "@iconify-json/logos": "^1.1.37",
    "@iconify-json/material-symbols": "^1.1.59",
    "@iconify-json/mdi": "^1.1.54",
    "@iconify-json/uil": "^1.1.5",
    "@nuxt/devtools": "1.0.0-beta.0",
    "@nuxt/image": "1.0.0-rc.3",
    "@nuxt/ui": "^2.9.0",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/i18n": "8.0.0-rc.5",
    "@nuxtjs/plausible": "^0.2.3",
    "@testing-library/vue": "^7.0.0",
    "@vernaillen/nuxt-base-layer": "^0.2.21",
    "@vitest/coverage-v8": "^0.34.6",
    "@vueuse/nuxt": "^10.4.1",
    "consola": "^3.2.3",
    "cypress": "^13.3.0",
    "eslint": "^8.50.0",
    "happy-dom": "11.2.0",
    "jsdom": "^22.1.0",
    "nuxt": "^3.7.4",
    "nuxt-multi-cache": "^3.1.1",
    "nuxt-vitest": "^0.11.0",
    "start-server-and-test": "^2.0.1",
    "typescript": "^5.2.2",
    "vitest": "0.33.0",
    "vue-tsc": "^1.8.15"
  }
}

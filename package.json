{
  "version": "1.3.1",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt build && nuxt preview",
    "postinstall": "nuxt prepare",
    "typecheck": "nuxi typecheck",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "cy:install": "cypress install",
    "cy:dev": "start-server-and-test dev http://localhost:3000 'cypress open --config baseUrl=http://localhost:3000'",
    "cy:local": "start-server-and-test preview http://localhost:3000 'cypress run --config baseUrl=http://localhost:3000'",
    "fulltest": "rm -rf node_modules && pnpm i && pnpm run lint && pnpm run coverage && pnpm run cy:local"
  },
  "devDependencies": {
    "@iconify-json/arcticons": "^1.1.85",
    "@iconify-json/circle-flags": "^1.1.45",
    "@iconify-json/game-icons": "^1.1.6",
    "@iconify-json/logos": "^1.1.40",
    "@iconify-json/material-symbols": "^1.1.65",
    "@iconify-json/mdi": "^1.1.59",
    "@iconify-json/uil": "^1.1.7",
    "@nuxt/devtools": "1.0.5",
    "@nuxt/image": "1.1.0",
    "@nuxt/test-utils": "latest",
    "@nuxt/ui": "^2.11.0",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/i18n": "8.0.0-rc.8",
    "@nuxtjs/plausible": "^0.2.4",
    "@vernaillen/nuxt-base-layer": "^0.2.21",
    "@vitest/coverage-v8": "^1.0.2",
    "@vueuse/nuxt": "^10.7.0",
    "cypress": "^13.6.1",
    "eslint": "^8.55.0",
    "happy-dom": "12.10.3",
    "jsdom": "^23.0.1",
    "nuxt": "^3.8.2",
    "nuxt-multi-cache": "^3.1.1",
    "nuxt-vitest": "^0.11.5",
    "playwright-core": "^1.40.1",
    "start-server-and-test": "^2.0.3",
    "typescript": "^5.3.3",
    "vitest": "1.0.2"
  }
}

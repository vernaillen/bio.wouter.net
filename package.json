{
  "version": "1.3.1",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt build && nuxt preview",
    "postinstall": "nuxt prepare",
    "typecheck": "nuxi typecheck",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "cy:install": "cypress install",
    "cy:dev": "start-server-and-test dev http://localhost:3000 'cypress open --config baseUrl=http://localhost:3000'",
    "cy:local": "start-server-and-test preview http://localhost:3000 'cypress run --config baseUrl=http://localhost:3000'",
    "fulltest": "rm -rf node_modules && pnpm i && pnpm run lint && pnpm run coverage && pnpm run cy:local"
  },
  "devDependencies": {
    "@iconify-json/arcticons": "^1.1.84",
    "@iconify-json/circle-flags": "^1.1.44",
    "@iconify-json/game-icons": "^1.1.5",
    "@iconify-json/logos": "^1.1.39",
    "@iconify-json/material-symbols": "^1.1.64",
    "@iconify-json/mdi": "^1.1.56",
    "@iconify-json/uil": "^1.1.6",
    "@nuxt/devtools": "1.0.3",
    "@nuxt/image": "1.1.0",
    "@nuxt/ui": "^2.11.0",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/i18n": "8.0.0-rc.5",
    "@nuxtjs/plausible": "^0.2.3",
    "@vernaillen/nuxt-base-layer": "^0.2.21",
    "@vitest/coverage-v8": "^1.0.0",
    "@vueuse/nuxt": "^10.6.1",
    "cypress": "^13.6.0",
    "eslint": "^8.54.0",
    "happy-dom": "12.10.3",
    "jsdom": "^22.1.0",
    "nuxt": "^3.8.2",
    "nuxt-multi-cache": "^3.1.1",
    "nuxt-vitest": "^0.11.5",
    "start-server-and-test": "^2.0.3",
    "typescript": "^5.3.2",
    "vitest": "0.33.0"
  }
}
